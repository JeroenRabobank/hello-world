<?xml version="1.0" encoding="UTF-8"?>
<sdkExtension
	xmlns="http://www.sap.com/bi/zen/sdk"
	id="com.sap.sample.video"
	title="Design Studio SDK Extension Sample Video"
	version="16.0"
	vendor="SAP">
	<component
		id="Video"
		title="Video"
		tooltip="Video"
    modes="commons m"    
		icon="res/video.png">
		<jsInclude>res/js/video.js</jsInclude>
		<property
			id="src"
			title="Video URL"
			type="Url"
			visible="false"
			bindable="true"/>
		<initialization>
			<defaultValue property="LEFT_MARGIN">40</defaultValue>
			<defaultValue property="TOP_MARGIN">40</defaultValue>
			<defaultValue property="WIDTH">300</defaultValue>
			<defaultValue property="HEIGHT">200</defaultValue>
		</initialization>
	</component>
</sdkExtension>
